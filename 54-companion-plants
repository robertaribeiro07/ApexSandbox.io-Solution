public Boolean companionPlants(String plant1, String plant2) {
  if( plant1 ='alface' == plant2='pepino'){
    return true;
  }
   if( plant1 ='alface' ==  plant2='cebola'){
    return true;
  }
   if(plant1 ='cebola' ==  plant2='cenoura'){
    return true;
  }
  if( plant1 ='cebola' ==  plant2='tomate'){
    return true;
  }else{
      return false;
  }
}